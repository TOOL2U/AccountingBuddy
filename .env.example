# Google Cloud Vision API Key
# Get your key from: https://console.cloud.google.com/apis/credentials
GOOGLE_VISION_KEY=your_vision_api_key

# OpenAI API Key (for AI extraction)
# Get your key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key

# Google Sheets Webhook URL (from Apps Script deployment - for Stage 3)
SHEETS_WEBHOOK_URL=https://script.google.com/macros/s/XXX/exec

# Webhook Secret (generate with: openssl rand -base64 32)
SHEETS_WEBHOOK_SECRET=your_long_random_string

# Mock mode for development (set to false in production)
NEXT_PUBLIC_MOCK=true

